{
  "name": "HackofLove",
  "description": "Hack of Love 2017",
  "version": "0.0.1",
  "private": true,
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "node dist",
    "prestart": "npm run -s build",
    "unit-local": "mocha src/test/** --compilers js:babel-core/register --reporter landing",
    "unit-local-debug": "mocha $NODE_DEBUG_OPTION src/test/** --compilers js:babel-core/register --reporter landing"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.15.0",
    "geolib": "^2.0.22",
    "morgan": "^1.8.0",
    "resource-router-middleware": "^0.6.0",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "mocha": "^3.2.0",
    "nodemon": "^1.9.2"
  }
}
